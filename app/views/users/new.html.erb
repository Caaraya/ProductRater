<%= form_for(@user) do |f| %>

  <%= f.label :name %>
  <%= f.text_field :name %>
  <br><br>

  <%= f.label :email %>
  <%= f.email_field :email %>

  <br>

  <%= f.label :password %>
  <%= f.password_field :password %>
  
  <br>

  <%= f.label :admin %>
  <%= f.check_box :admin %>
  
  <br>

  <%= f.submit %>

<% end %>